<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">

        <record id="ticket_view_form" model="ir.ui.view">
            <field name="name">helpdesk.ticket.inherit.view.form</field>
            <field name="model">helpdesk.ticket</field>
            <field name="inherit_id" ref="helpdesk.ticket_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='data']" position="inside">
                    <field name="picking_ids"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
